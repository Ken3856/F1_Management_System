#include<iostream>
#include<string>
using namespace std;

class Vehicle {
protected:
    string brand;
    string color;
    int maxSpeed;

public:
    Vehicle(string brand, string color, int maxSpeed)
        : brand(brand), color(color), maxSpeed(maxSpeed) {}

    virtual void getEngine() const = 0;

    virtual string getType() const = 0;

    virtual ~Vehicle() {} 

   
    string getBrand() const { return brand; }
    string getColor() const { return color; }
    int getMaxSpeed() const { return maxSpeed; }
};
